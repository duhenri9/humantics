      "parameters": {
        "method": "POST",
        "url": "http://localhost:5000/api/mcp/agent/mcp_personalizado_1751833568572/message",
        "sendHeaders": true,
        "headerParameters": {
          "parameters": [
            {
              "name": "Content-Type",
              "value": "application/json"
